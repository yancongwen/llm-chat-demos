{
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "node",
    "declaration": false,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es6",
    "sourceMap": false,
    "outDir": "dist",
    "baseUrl": "./"
  },
  "exclude": ["node_modules"]
}